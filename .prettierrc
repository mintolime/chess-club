{
  "tabWidth": 2,
  "useTabs": true,
  "printWidth": 120,
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "parser": "html",
        "singleQuote": false
      }
    }
  ]
}